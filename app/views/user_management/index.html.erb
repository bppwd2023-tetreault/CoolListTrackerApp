<h1>Users</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |u| %>
      <tr>
        <th scope="row"> <%= u.id %> </th>
        <td><a href="mailto:<%= u.email %>"> <%= u.email %> </a> </td>
      </tr>
    <% end %>
  </tbody>
</table>