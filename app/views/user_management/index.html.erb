<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |u| %>
      <% if u != current_user %> 
        <tr>
          <td> <%= link_to u.name, user_management_show_path(id:u.id) %> </td>
          <td> <%= u.email %> </td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>