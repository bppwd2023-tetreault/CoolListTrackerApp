<p style="color: green"><%= notice %></p>

<h1>Lists</h1>

<div id="lists">
  <% @lists.each do |list| %>
    <% if !list.archived %>
      <%= render list %>
      <p>
        <%= link_to "Show this list", list, class: "btn btn-primary" %>
      </p>
    <% end %>
  <% end %>
</div>

<%= link_to "New list", new_list_path, class: "btn btn-success my-4" %>


